[Unit]
Description=The Eucalyptus Cluster Controller service
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
Type=forking
PIDFile=/var/run/eucalyptus/eucalyptus-cc.pid
# EnvironmentFile=/etc/sysconfig/eucalyptus
ExecStart=/bin/sh -x /usr/sbin/eucalyptus-cc.init start
ExecStop=/bin/sh -x /usr/sbin/eucalyptus-cc.init stop
PrivateTmp=false

[Install]
WantedBy=multi-user.target
