[Unit]
Description=The Eucalyptus Cloud web services stack
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
Type=forking
PIDFile=/var/run/eucalyptus/eucalyptus-cloud.pid
EnvironmentFile=/etc/sysconfig/eucalyptus
ExecStart=/bin/sh -x /usr/sbin/eucalyptus-cloud.init start
ExecStop=/bin/sh -x /usr/sbin/eucalyptus-cloud.init stop
PrivateTmp=false

[Install]
WantedBy=multi-user.target
